<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">



<struts-config>

    <form-beans>
        <form-bean
            name="relacaoForm"
            type="com.mobi.relacao.form.RelacaoForm"/>
    </form-beans>


    <action-mappings>
            
		<action path="/ajaxAddRelacao" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" parameter="addRelacoesInstancia" scope="session">
		</action>
		
		<action path="/ajaxDiagrama" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" parameter="salvarRelacoes" scope="session">
			<forward name="success" path="/pages/ajaxDiagrama.jsp"/>
		</action>
		
		<action path="/eliminarRelacaos" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" parameter="eliminarRelacionamentoClasse" scope="session">
			<forward name="success" path="/pages/ajaxDiagrama.jsp"/>
		</action>
		
		
		<action path="/gerarArquivoOWL" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" parameter="gerarArquivoOWL" scope="session">
		</action>
		
		<action path="/modelagem" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" scope="session" parameter="visualizarRelacoes">
			<forward name="success" path="/pages/editorMobi.jsp"/>
		</action>
		
		<action path="/ajaxAddInstancia" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" scope="session" parameter="addInstanciaRelacao">
		</action>
		
		<action path="/ajaxEditarNomeInstancia" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" scope="session" parameter="atualizarNomeInstancia">
		</action>
		
		<action path="/ajaxListarRelacoes" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" scope="session" parameter="listarRelacoes">
				<forward name="success" path="/pages/listarRelacoes.jsp"/>
		</action>
		
		<action path="/ajaxCarregarRelacao" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" scope="session" parameter="carregarRelacao">
				<forward name="success" path="/pages/ajaxCarregarRelacao.jsp"/>
		</action>
		
		<action path="/ajaxBuscarInstancia" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" scope="session" parameter="atualizarInstancias">
				<forward name="success" path="/pages/ajaxCarregarRelacao.jsp"/>
		</action>
		
		<action path="/ajaxEliminarRelacionamento" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" scope="session" parameter="eliminarRelacionamentoInstancia" >
		</action>
		
		<action path="/ajaxAlterarNomeInstancia" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" scope="session" parameter="alterarNomeInstancia" >
		</action>
		
		<action path="/ajaxEliminarInstancia" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" scope="session" parameter="eliminarInstancia" >
				<forward name="success" path="/pages/ajaxCarregarRelacao.jsp"/>
		</action>
		
		<action path="/cleanRelation" type="com.mobi.comum.action.EditorMMobiAction" 
				name="relacaoForm" scope="session" parameter="cleanRelationGeneric" >
		</action>
		
		<action path="/diagramaInit" forward="/pages/ajaxDiagrama.jsp"/>
		
		<action path="/atualizarNomeClasse" type="com.mobi.comum.action.EditorMMobiAction"
				name="relacaoForm" scope="session" parameter="atualizarNomeClasse">
				<forward name="success" path="/pages/ajaxDiagrama.jsp"/>
		</action>
		
		<action path="/addClass" type="com.mobi.comum.action.EditorMMobiAction"
				name="relacaoForm" scope="session" parameter="addClasse">
				<forward name="success" path="/pages/ajaxDiagrama.jsp"/>
		</action>
		
		<action path="/cleanSession" type="com.mobi.comum.action.EditorMMobiAction"
				name="relacaoForm" scope="session" parameter="cleanSession">
				<forward name="success" path="/pages/ajaxDiagrama.jsp"/>
		</action>
		
		<action path="/salvarEstadoMobi" type="com.mobi.comum.action.EditorMMobiAction"
				name="relacaoForm" scope="session" parameter="salvarEstadoMobi">
		</action>
		
		<action path="/recuperarMobi" type="com.mobi.comum.action.EditorMMobiAction"
				name="relacaoForm" scope="session" parameter="recuperarEstado">
				<forward name="success" path="/pages/ajaxDiagrama.jsp"/>
		</action>
		
		<action path="/recuperarMobi" type="com.mobi.comum.action.EditorMMobiAction"
				name="relacaoForm" scope="session" parameter="recuperarEstado">
				<forward name="success" path="/pages/ajaxDiagrama.jsp"/>
		</action>
		
		<action path="/uploadOWL" type="com.mobi.comum.action.EditorMMobiAction"
				name="relacaoForm" scope="session" parameter="uploadOWL">
				<forward name="success" path="/pages/envio.jsp"/>
		</action>
		
    </action-mappings>



    <message-resources parameter="MessageResources" />


  <plug-in className="org.apache.struts.validator.ValidatorPlugIn">
    <set-property
        property="pathnames"
        value="/org/apache/struts/validator/validator-rules.xml,
               /WEB-INF/validation.xml"/>
  </plug-in>

</struts-config>

